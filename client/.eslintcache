rofile:payload\n            }\n         case PROFILE_ERROR:\n                return {\n                    ...state,\n                    loading:false,\n                    error:payload,\n                    // profile:null sakshi logic\n                };\n        case CLEAR_PROFILE:\n                return{\n                    ...state,\n                    profile:null,\n                    loading:false\n                }\n\n        default:\n                return state\n    }\n\n}","/Users/sakshi/Desktop/fin/client/src/actions/profile.js",[],"/Users/sakshi/Desktop/fin/client/src/components/profile-forms/CreateProfile.js",["107","108"],"/Users/sakshi/Desktop/fin/client/src/components/dashboard/DashboardActions.js",[],"/Users/sakshi/Desktop/fin/client/src/components/profile-forms/EditProfile.js",["109","110","111"],"import React,{Fragment,useState,useEffect} from 'react'\nimport {Link,withRouter} from 'react-router-dom'\nimport PropTypes from 'prop-types'\nimport {connect } from 'react-redux';\nimport {createProfile,getCurrentProfile} from '../../actions/profile'\nconst  EditProfile=({profile:{profile,loading},createProfile,getCurrentPROFILE ,history})=>{\n    const [formData,setFormData]=useState({\n       location:'',\n       status:'',\n       bio:'',\n     });\n     useEffect(()=>{\n         getCurrentProfile();\n         setFormData({\n             location:loading||!profile.location?'':profile.location,\n             status:loading||!profile.status?'':profile.status,\n             bio:loading||!profile.bio?'':profile.bio,\n             youtube:loading||!profile.social?'':profile.social.youtube,\n             instagram:loading||!profile.social?'':profile.social.instagram,\n         })\n     },[loading])\n    const {\n        location,\n        status,\n        bio\n\n    } =formData;\n    const onChange=e=>setFormData({...formData,[e.target.name]:e.target.value});\n    const onSubmit=e=>{\n        e.preventDefault();\n        createProfile(formData,history,true);\n    }\n    return (\n        <Fragment>\n             <h1 className=\"large text-primary\">\n        Create Your Profile\n      </h1>\n      <p className=\"lead\">\n        <i className=\"fas fa-user\"></i> Let's get some information to make your\n        profile stand out\n      </p>\n      <small>* = required field</small>\n      <form className=\"form\" onSubmit={e=>onSubmit(e)}>\n        <div className=\"form-group\">\n       \n          <small className=\"form-text\"\n            >Give us an idea of where you are at in your career</small\n          >\n        </div>\n        <div className=\"form-group\">\n          \n        </div>\n        <div className=\"form-group\">\n         \n        </div>\n        <div className=\"form-group\">\n          <input type=\"text\" placeholder=\"Location\" name=\"location\"  value={location} onChange={e=>onChange(e)}/>\n          <small className=\"form-text\"\n            >City & state suggested (eg. Boston, MA)</small\n          >\n        </div>\n        <div className=\"form-group\">\n          \n        </div>\n        <div className=\"form-group\">\n         \n          \n        </div>\n        <div className=\"form-group\">\n          <textarea placeholder=\"A short bio of yourself\" name=\"bio\" value={bio} onChange={e=>onChange(e)}></textarea>\n          <small className=\"form-text\">Tell us a little about yourself</small>\n        </div>\n\n        <div className=\"my-2\">\n          <button type=\"button\" className=\"btn btn-light\" >\n            Add Social Network Links\n          </button>\n          <span>Optional</span>\n        </div>\n\n        \n\n      \n\n        <div className=\"form-group social-input\">\n          <i className=\"fab fa-youtube fa-2x\"></i>\n          <input type=\"text\" placeholder=\"YouTube URL\" name=\"youtube\" onChange={e=>onChange(e)} />\n        </div>\n\n        \n\n        <div className=\"form-group social-input\">\n          <i className=\"fab fa-instagram fa-2x\"></i>\n          <input type=\"text\" placeholder=\"Instagram URL\" name=\"instagram\" onChange={e=>onChange(e)} />\n        </div>\n        <input type=\"submit\" className=\"btn btn-primary my-1\" />\n        \n      </form>\n\n        </Fragment>\n\n    )\n}\nEditProfile.propTypes={\n    EditProfilefile:PropTypes.func.isRequired,\n    getCurrentProfile:PropTypes.func.isRequired,\n    profile:PropTypes.object.isRequired\n}\nconst mapStateToProps=state=>({\n    profile:state.profile\n})\n\nexport default connect(mapStateToProps,{createProfile,EditProfile,getCurrentProfile})(withRouter(EditProfile))",{"ruleId":"112","replacedBy":"113"},{"ruleId":"114","replacedBy":"115"},{"ruleId":"116","severity":1,"message":"117","line":5,"column":8,"nodeType":"118","messageId":"119","endLine":5,"endColumn":14},{"ruleId":"120","severity":1,"message":"121","line":3,"column":1,"nodeType":"122","endLine":16,"endColumn":2},{"ruleId":"116","severity":1,"message":"123","line":1,"column":19,"nodeType":"118","messageId":"119","endLine":1,"endColumn":31},{"ruleId":"124","severity":1,"message":"125","line":13,"column":7,"nodeType":"126","endLine":13,"endColumn":9,"suggestions":"127"},{"ruleId":"120","severity":1,"message":"121","line":8,"column":1,"nodeType":"122","endLine":35,"endColumn":2},{"ruleId":"116","severity":1,"message":"128","line":2,"column":9,"nodeType":"118","messageId":"119","endLine":2,"endColumn":13},{"ruleId":"116","severity":1,"message":"129","line":14,"column":9,"nodeType":"118","messageId":"119","endLine":14,"endColumn":15},{"ruleId":"116","severity":1,"message":"128","line":2,"column":9,"nodeType":"118","messageId":"119","endLine":2,"endColumn":13},{"ruleId":"124","severity":1,"message":"130","line":21,"column":8,"nodeType":"126","endLine":21,"endColumn":17,"suggestions":"131"},{"ruleId":"116","severity":1,"message":"129","line":24,"column":9,"nodeType":"118","messageId":"119","endLine":24,"endColumn":15},"no-native-reassign",["132"],"no-negated-in-lhs",["133"],"n